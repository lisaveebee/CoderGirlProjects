import static java.lang.System.out;
import java.util.Scanner;

public class Experiment
{
    public static void main(String[] args) 
    {
        double mySmallNumber = 0.12345678901234567890123456789;
        out.println("My number is " + mySmallNumber);
        
        out.println("");
        out.println("next program");
        long myBigNumber = 999999999;
        out.println("My number is " + myBigNumber);
        
        out.println("");
        out.println("next program");
        out.println("4 times 3 is " + (4 * 3));
        // 9 zeros
        int bigA = 1000000000;
        int bigB = 1000000000;
        out.println("What is bigA times bigB? ");
        out.println(bigA * bigB);
        
        out.println("");
        out.println("next program");
        int appleCount = 3;
        out.println("I have " + appleCount + " apples.");
