        out.println("Now I have " + appleCount + " apples.");
        appleCount = appleCount - 1;
        out.println("Now I have " + appleCount + " apples.");
        appleCount = appleCount - 1;
        out.println("Now I have " + appleCount + " apples.");
        appleCount = appleCount - 1;
        out.println("Now I have " + appleCount + " apples.");
        appleCount = appleCount - 1;
        out.println("Now I have " + appleCount + " apples.");
        appleCount = appleCount - 1;
        out.println("Now I have " + appleCount + " apples.");
        
        out.println("");
        out.println("next program");
        appleCount = 4;
        out.println("I have " + appleCount + " apples.");
