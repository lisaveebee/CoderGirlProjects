        out.println(someletter);

        out.println("");
        out.println("next program");
        int letter1 = (int) 'a';
        int letter2 = (int) 'b';
        int letter3 = (int) 'c';
        int letter4 = (int) 'd';
        int letter26 = (int) 'z';
        int cap1 = 'A';
        int cap2 = 'B';
        int cap3 = 'C';
